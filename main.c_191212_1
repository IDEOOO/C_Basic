#include <stdio.h>

void squareref(int *ptr)
{
    *ptr*=*ptr;
}

int main(void)
{
    int num;
    
    printf("num: ");
    scanf("%d", &num);
    
    squareref(&num);
    
    printf("result: %d \n", num);
    
    return 0;
}
